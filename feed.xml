---
title:      t.pitale blog
extension: xml
layout: false
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>t.pitale</title>
  <link href="http://t.pitale.com/feed.xml" rel="self" />
  <link href="http://t.pitale.com/" />
  <id>http://t.pitale.com/</id>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <author>
    <name>Tony Pitale</name>
    <email>tpitale@gmail.com</email>
  </author>

  {% assign posts = site.posts %}
  {% for post in posts limit:10 %}
    <entry>
      <id>{{post.url}},{{post.created_at}}</id>
      <title>{{post.title}}</title>
      <subtitle>{{post.subtitle}}</subtitle>
      <link href="http://t.pitale.com/{{post.url}}" />
      <updated>{{post.created_at | date_to_xmlschema}}</updated>
    </entry>
  {% endfor %}
</feed>