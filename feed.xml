---
extension: xml
layout: false
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>tpitale</title>
  <link href="http://feeds.feedburner.com/tpitale" rel="self" />
  <link href="http://tpitale.com/" />
  <id>http://tpitale.com/</id>
  <updated>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</updated>
  <author>
    <name>Tony Pitale</name>
    <email>tpitale@gmail.com</email>
  </author>

  {% for post in site.posts limit:10 %}
    <entry>
      <id>{{post.id}}</id>
      <title>{{post.title}}</title>
      <subtitle>{{post.subtitle}}</subtitle>
      <link href="http://tpitale.com{{post.url}}" />
      <updated>{{post.created_at | date: "%a, %d %b %Y %H:%M:%S %z" }}</updated>
      <content type="html">
        {{post.content | xml_escape}}
      </content>
    </entry>
  {% endfor %}
</feed>
